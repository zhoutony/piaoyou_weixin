.item-mask {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 1000;
  background: rgba(0, 0, 0, 0.5);
  display: -webkit-flex;
  display: flex;
  -webkit-flex-direction: column;
  flex-direction: column;
}

.item-mask .mask-close {
  height: 80rpx;
  text-align: center;
  color: white;
}

.item-mask .mask-content {
  -webkit-flex: 1;
  flex: 1;
  background: white;
  border-top: 10rpx solid #00968e;
}

.mask-header {
  border-bottom: 1rpx solid #e8e8e8;
  padding: 0 0 20rpx 0;
  position: relative;
}

.mask-header .btn-link {
  position: absolute;
  right: 40rpx;
  top: 35rpx;
}

.mask-header .tips, .mask-header .title {
  display: block;
  text-align: center;
}

.mask-header .tips {
  height: 60rpx;
  line-height: 60rpx;
  color: #666;
  text-align: center;
}

.mask-header .logo {
  width: 120rpx;
  height: 120rpx;
  margin: 10rpx auto;
  background: orange;
  -webkit-border-radius: 1000rpx;
  border-radius: 1000rpx;
}

.mask-header .title {
  color: #00968e;
}

.formlist {
  padding: 30rpx;
}

.formlist .form-box {
  border: 1rpx solid #ddd;
  height: 68rpx;
  margin: 20rpx 0 0;
}

.formlist .form-box  input {
  height: 68rpx;
  display: block;
  padding: 0 10rpx;
}

.formlist .form-btns {
  margin-top: 50rpx;
  padding: 0 30rpx;
}

.formlist   .form-error {
  border-color: red;
}

.formlist   .form-error input {
  color: red;
}

.formlist .form-btns button {
  -webkit-border-radius: 0;
  border-radius: 0;
  height: 68rpx;
  line-height: 68rpx;
  font-size: 24rpx;
}

.pushcode .btn-code {
  height: 68rpx;
  line-height: 68rpx;
  width: 220rpx;
  margin-left: 20rpx;
  -webkit-border-radius: 0;
  border-radius: 0;
}

.formlist .pushcode {
  margin-top: 20rpx;
}

.formlist .pushcode .form-box {
  margin: 0;
}

.formlist .error-text {
  display: block;
  color: red;
  text-align: center;
  font-size: 24rpx;
  height: 60rpx;
  line-height: 68rpx;
}

.formlist .codeimg {
  margin: 0 20rpx;
  width: 200rpx;
  border: 1rpx solid #e8e8e8;
  height: 68rpx;
  background: white;
}

.formlist .info-txt {
  font-size: 24rpx;
  padding: 30rpx 0 0;
  display: block;
}

.order-tab {
  display: block;
  border: 1rpx solid #e8e8e8;
  color: #00968e;
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;
  margin: 30rpx 20rpx 0rpx;
  -webkit-border-radius: 100rpx;
  border-radius: 100rpx;
  font-size: 24rpx;
}

.item-orderbox {
  background: #00968e;
  color: white;
  margin: 0 20rpx 20rpx;
  padding: 20rpx;
  position: relative;
}
.item-orderbox:before,
.item-orderbox:after{
display: block;
  background: #f65427;
  color: #fff;
  position: absolute;
  top: 0;
  padding:0rpx 20rpx;
  height:40rpx;
  line-height:40rpx;
}
.item-orderbox:after {
  content: '未放映';
  right: 0;
  font-size: 22rpx;
}
.item-orderbox:before{
  content:'';
  display:block;
  right:80rpx;
  -webkit-transform:skew(20deg,0deg);
  transform:skew(20deg,0deg);
}


.item-orderbox text {
  font-size: 24rpx;
  line-height: 1.4;
}

.item-orderbox .code {
  background: #999;
  margin: 10rpx 0;
  padding: 10rpx;
  width: 60%;
}
.disabledorder,
.disabledorder .code {
  background: #b8b8b8;
}
.disabledorder:before,
.disabledorder:after{
background: #888;
}
.disabledorder:after {
  
  content:'已放映';
}

.disabledorder .code {
  padding-left: 0;
}
.item-orderbox .code text {
  display: block;
}

.item-card {
  margin: 0 20rpx 20rpx;
  border: 1rpx solid #00968e;
  -webkit-border-radius: 30rpx;
  border-radius: 30rpx;
  padding: 20rpx;
}

.item-card .price {
  padding: 0 0 20rpx 0;
  border-bottom: 1px dashed #e8e8e8;
}

.item-card .price  text {
  font-weight: bold;
}

.item-card .price .txt {
  color: #00968e;
  font-weight: normal;
}

.item-card .other text {
  display: block;
  font-size: 22rpx;
  color: #c6c6c6;
  padding: 15rpx 0 0;
}

.red-bag{
    padding:20rpx;
}

.red-box{
    position:relative;
    background:url(data:image/png;base64,) repeat-x 0 0;
    background-size:auto 29rpx; 
    margin-bottom:20rpx;
}

.red-box .main-box{
    padding:50rpx 0 20rpx;
    position:relative;
}
.red-box:after{
    width:0;
    height:0;
    content:'';
    display:block;
   border-top: 10rpx solid transparent;
    border-right: 10rpx solid transparent;
    border-bottom: 10rpx solid transparent;
    border-left: 10px solid #00968e;
    position:absolute;
    
    z-index: 10;
    right:0;
    top:50%;
    margin-top:-30rpx;
     
}
.red-box .bar-block{
padding-right:50rpx;
}
.red-box .main-box .tips{
    position:absolute;
    right:40rpx;
    top:40rpx;
    width:120rpx;
    height:120rpx;
    line-height:120rpx;
    border:1px solid #e8e8e8;
    font-size:100rpx;
    text-align:center;color:#e8e8e8;
    -webkit-border-radius:1000rpx;
    border-radius:1000rpx;
    z-index: 0;
}
.red-box .main-box text{
    display:block;
    font-size:22rpx;
    padding:0 0 10rpx 0;
    z-index: 1;
    position:relative;
}
.red-box .main-box  .con-block text{
    color:#00968e;
}
.red-box .main-box .bar-block text{
    text-align:right;
}
.red-box .other-box{
    border-top:1rpx solid #e8e8e8;
    height:60rpx;
}
.red-box .other-box text{
    font-size:22rpx;
}
.red-box .red-price{
    min-height:80rpx;
}
.red-box .red-price .big{
    font-size:38rpx;
}
.red-box .red-price text{
    font-size:28rpx;
}
.red-disabled text{
    color:#888;
}
.red-disabled .main-box .con-block text{
    color:#ccc;
}
.red-disabled{
    background:url(data:image/png;base64,) repeat-x 0 0;
    background-size:auto 29rpx;
}
.red-disabled:after{
    border-left-color:#d8d8d8;
}
 