.mask-default{
    position:fixed;
    top:0;
    right:0;
    bottom:0;
    left:0;
    background:rgba(0,0,0,.8);
    z-index: 1000;
   
}

.mask-default{
 display:-webkit-flex;
    display:flex;
    -webkit-align-items:center;
    align-items:center;
}


.mask-box{
    width:100%;
    background:white;
    margin:0 50rpx;
    min-height:88rpx;
    -webkit-border-radius:6rpx;
    border-radius:6rpx;
    
}
.mask-hd,
.mask-bd{
    height:88rpx;
}
.mask-hd {
    text-align:center;
}
.mask-hd .title{
    line-height:88rpx;
    font-size:34rpx;
}
.mask-bd{
    border-top:1rpx solid #e8e8e8;
}
.mask-con{
    padding:30rpx 30rpx 60rpx;
}
.mask-bd .item-flex{
    line-height:88rpx;
    text-align:center;
    font-size:34rpx;
    
}
.mask-bd .line{
    background:#e8e8e8;
    width:1rpx;
    overflow:hidden;
}
.content-txt{
    text-align:center;
    display:block;
}
.mask-bd .btn-color{
   color:#00968d; 
}
