/**item-cinema**/

.item-cinema {
  border-top: 1rpx solid #e5e5e5;
}

.item-cinema .name {
  font-size: 30rpx;
}

.item-cinema .address {
  font-size: 24rpx;
}

.item-cinema .item-label {
  height: 30rpx;
  line-height: 30rpx;
  padding: 0 10rpx;
  font-size: 20rpx;
 
}

.item-schedule {
  border-bottom: none;
  margin: 0 20rpx;
  padding: 0;
}

.item-schedule .schedule {
  border-bottom: 1rpx dashed #e5e5e5;
  padding: 30rpx 0rpx;
}

.item-schedule  .item-bar {
  padding-right: 30rpx;
}

.item-schedule .item-tool {
  padding-left: 30rpx;
  text-align: right;
}

.item-schedule  .start-time, .item-schedule  .end-time,
.item-schedule  .language, .item-schedule  .theater {
  display: block;
}

.item-schedule  .start-time {
  font-size: 48rpx;
  font-weight: bold;
}

.item-schedule  .end-time {
  padding-top: 20rpx;
  color: #888;
}

.item-schedule  .language {
  color: #888;
  padding-top: 10rpx;
}

.item-schedule  .theater {
  color: #888;
  padding-top: 30rpx;
}

.item-schedule .price {
  text-align: right;
}

.item-schedule  .oldprice, .item-schedule  .newprice {
  display: block;
}

.item-schedule  .oldprice {
  color: #898989;
  text-decoration: line-through;
  padding-top: 10rpx;
}

.item-schedule  .newprice {
  padding-top: 26rpx;
  color: #4DB3AE;
  font-size: 32rpx;
}

.item-schedule .item-label {
  display: block;
  text-align: center;
  margin-right: 0;
  border-color:#4DB3AE;
  background:#4DB3AE;
  color:#fff;
}

.cinematips {
  background: #fcf8e8;
  height: 68rpx;
  line-height: 68rpx;
  padding: 0 30rpx;
  color: #ed8e1e;
}

.cinematips text {
  font-size: 24rpx;
}

.item-time {
  height: 88rpx;
  position: relative;
}

.item-time:after, .item-time .time:after {
  content: '';
  position: absolute;
  left: 0;
  right: 0;
  overflow: hidden;
}

.item-time:after {
  background: #e5e5e5;
  bottom: 0;
  z-index: 1;
  height: 2rpx;
}

.item-time view {
  height: 100%;
}

.item-time .time {
  display: block;
  padding: 0 20rpx;
  position: relative;
  height: 100%;
  line-height: 88rpx;
  min-width: 140rpx;
  text-align: center;
}

.item-time .time:after {
  bottom: 0rpx;
  height: 4rpx;
  background: #4DB3AE;
  display: none;
  z-index: 2;
}

.item-time .curr {
  color: #4DB3AE;
  font-weight: bold;
}

.item-time .curr:after {
  display: block;
}

.showmovie {
  padding: 30rpx 20rpx;
}

.showmovie .movie-dt {
  padding-bottom: 20rpx;
}

.showmovie .movie-dt .name {
  font-size: 34rpx;
  margin-right: 20rpx;
}

.showmovie .movie-dd .ohtertxt {
  font-size: 24rpx;
  color: #888;
}

.movieimglist {
  margin: 0rpx 20rpx 0rpx 20rpx;
  width: auto;
}

.movieimglist .item-flexbox-v-e {
  padding: 30rpx 0;
  min-height: 256rpx;
  -webkit-box-sizing:border-box;
  box-sizing:border-box
}

.movieimglist .img-box {
  width: 140rpx;
  height: 196rpx;
  background: none;
  position: relative;
  margin-right: 6rpx;
}

.movieimglist .curr:before, .movieimglist .img-box:after {
  content: '';
  position: absolute;
  display: block;
}

.movieimglist .img-box:after {
  display: none;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  border: 6rpx solid #4DB3AE;
}

.movieimglist .curr:before {
  width: 0;
  height: 0;
  border-left: 8px solid transparent;
  border-right: 8px solid transparent;
  border-top: 16rpx solid #4DB3AE;
  left: 50%;
  bottom: -16rpx;
  margin-left: -8rpx;
  z-index: 4;
}

.movieimglist .img-box image {
  transform: scale(0.8);
  vertical-align: bottom;
}

.movieimglist .curr:after {
  display: block;
}

.movieimglist .curr, .movieimglist .curr image {
  transform: scale(1);
}

/** schedule.wxss **/

.m-hide {
  display: none;
}

